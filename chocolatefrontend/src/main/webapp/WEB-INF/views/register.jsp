<div class="row">
                    
<div class="col-md-12">
                        
<div class="section" style="padding-bottom:150px">
                            
<div class="container">
                                
<div class="row">
                                    
<div class="col-lg-4"></div>
                                    
<div class="col-lg-4">
                                        
<form class="form-horizontal" action="Register" method="POST">
   
<font size="6"><b> REGISTERATION FORM </b></font>
<br>
<br>
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="firstname">FIRST NAME</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="firstname" placeholder="FIRST NAME"  required="required">
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="lastname">LAST NAME</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="lastname" placeholder="LAST NAME" required="required" >
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="gender">GENDER</label>
                                                
</div>
                                                
 <div class="col-sm-7">
                                                    
MALE<input type="radio" name=" "> FEMALE<input type="radio" name=" " required="required" >                                              
</div>
                                                      
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="dateofbirth">DATE OF BIRTH</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="date" id="dateofbirth" placeholder="25/11/1994" required="required" >
                                                
</div>
                                            
</div>                  
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="emailid">EMAIL ID</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="emailid" placeholder="jeni@gmail.com" required="required" >
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="address">ADDRESS</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="address" placeholder=" " required="required" >
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="city">CITY</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="city" placeholder=" CHENNAI" required="required">
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="contactnumb">CONTACT NUMBER</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="number" id="contactnumb" placeholder="+91 9876543210" required="required" >
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="password">PASSWORD</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="password" placeholder=" " required="required" >
                                                
</div>
                                            
</div>
        
<div class="form-group">
  
<div class="col-sm-5">
                                                    
<label for="conformpassword">CONFORM PASSWORD</label>
                                                
</div>
                                                
<div class="col-sm-7">
                                                    
<input type="text" id="conformpassword" placeholder=" " required="required">
                                                
</div>
                                            
</div>
        
<div class="form-group">
                                                
<div class="col-sm-offset-2 col-sm-10">
                                                    
<button type="submit" class="btn btn-default">SUBMIT</button>
  
